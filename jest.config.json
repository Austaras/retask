{
    "testMatch": ["<rootDir>/test/**/*.ts"],
    "transform": {
        "^.+\\.m?[t|j]sx?$": "babel-jest"
    },
    "coverageReporters": ["lcov", "text"],
    "collectCoverageFrom": ["lib/es6/src/*.js"],
    "transformIgnorePatterns": ["node_modules/(?!(@rescript\/std)/)"],
    "moduleFileExtensions": ["js", "jsx", "ts", "mjs"]
}
